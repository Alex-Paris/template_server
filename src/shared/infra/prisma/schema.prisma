generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id         String   @id
  name       String
  email      String   @unique
  password   String   @ignore
  avatar     String?
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}
